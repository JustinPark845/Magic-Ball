{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "outDir": "build/dist",
    "module": "esnext",
    "target": "es5",
    "lib": [
      "es6",
      "dom"
    ],
    "sourceMap": true,
    "allowJs": true,
    "jsx": "react",
    "moduleResolution": "node",
    "rootDir": "ReactNativeTS_Learning",
    "strict": true,
    "allowSyntheticDefaultImports": true,
    "resolveJsonModule": true
  }
}
